void main(){
    int a,b;									/*multiple decls*/
    int c,d,e;
    int var_1, var_2;								/*variable name with underscore*/
    int var3 ,var4;
    a=100;									/*checking expressions*/
    a=b+10;
	if(var3 <> var4){
		var3= var3+ 1;
	}
	else
	{
		var3=var4;
	}
    while(a >= b){								/*iterator block*/
        a=b+10;									/*nested if -else*/
        if(a <= 30){								/*selector block*/
            if(var1 <> var2){							/*another selector block*/
                var1= var2-1;
            }
            else
            {
                var3= var3-1;
            }
            a=b-5;
        }
        else{
            a=b-1;
        }
        if(b > 20){
            b=b-1;
        }
        else
        {
            c=c-1;
        }
        a=b+1;
    }
    b=a+b;
    if(c == 20){
        c=20-1;
    }
    else
    {
		c=100;
    }
}
