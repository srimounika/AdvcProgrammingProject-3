       	remainder = n + 10;
        sum = sum + remainder;															
	temp_1=var_1;						
   	temp_2=var_2;
    	while(temp_1 > temp_2){
       		if(temp_1 >= temp_2){
            		temp_1= temp_1+temp_2;
        	}
       		else{
           		temp_2= temp_2 - temp_1;
        	}									
    	}	
    }
    read(a);
