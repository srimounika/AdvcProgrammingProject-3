  @purpose     		: This program is to check condition comparison operation , and execute statements inside if while and assignment
*/
void main(){		
    	int a,b,c,n;
	read(a);
	read(b);
	read(c);
	read(n);
	if(a == b){
		a= a-1;
	}
	else
	{
		a=a-b;
	}
	while(n > 1){
		n= n-1;
		a=a+b;
		b=b+1;
		c=a-b;
	}
	if(c == 10){
		write(c);
	}
	else
	{	
		write(b);
	}
	write(a);
}
